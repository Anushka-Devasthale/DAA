
# find all pairs of numbers whose sum is equal to a particular number

def SumPair(array, targetSum):
    i = 0
    j = i+1
    for i in range(len(array)-1):
        for j in range(len(array)-1):
            if(array[i])+(array[j])==targetSum:
                print(array[i], array[j])
            else:
                pass
    
array = [1,3,4,6,7,8,2,9,10]
SumPair(array, 4)
